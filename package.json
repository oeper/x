{
      "name": "twitter-clone",
      "version": "1.0.0",
      "description": "A basic Twitter clone using React and Node.js",
      "main": "server/server.js",
      "scripts": {
        "start": "node server/server.js",
        "client": "cd client && npm start",
        "server": "nodemon server/server.js",
        "dev": "concurrently \"npm run server\" \"npm run client\""
      },
      "dependencies": {
        "express": "^4.17.1",
        "cors": "^2.8.5"
      },
      "devDependencies": {
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.4"
      }
    }
